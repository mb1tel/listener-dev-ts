# Environment Configuration (.env.sample)
# Copy to .env and modify as needed

# Node Environment: development, production, test
NODE_ENV=development

# Server Configuration
PORT=3000

# Redis Configuration
REDIS_MODE=cluster # standalone or cluster
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_USERNAME=
REDIS_PASSWORD=bitnami

# Application Settings
LOG_LEVEL=info
CORS_ORIGIN=*

# Security
SECRET_KEY=your_super_secret_socket_key_2024

# Development Settings (only used in development)
DEBUG=false
ENABLE_METRICS=false 